<h1 style="color:blue;">Ingrese datos</h1>
<br>

<form id="formProds">

    <div class="form-group">
        <label for="nombre"><b>Nombre del producto</b></label>
        <input id="nombre" class="form-control" type="text" name="nombre">
    </div>

    <div class="form-group">
        <label for="precio"><b>Precio</b></label>
        <input id="precio" class="form-control" type="number" name="precio">
    </div>

    <div class="form-group">
        <label for="imagen"><b>Imagen</b></label>
        <input id="imagen" class="form-control" type="text" name="imagen">
        
    </div>

    <button type="submit" class="btn btn-success mt-3 mb-5">Enviar</button>
</form>

<script src="./js/form.js"></script>